<odoo>

	<record id="qms_wizard_view" model="ir.ui.view">
		<field name="name">queue.message.wizard</field>
		<field name="model">queue.message.wizard</field>
		<field name="type">form</field>
		<field name="arch" type="xml">
			<form string="Message" version="7.0">
				<separator string="Message" colspan="6"/>
				<field name="text" colspan="4" nolabel="1"/>
				<newline/>
				<separator colspan="6"/>
				<footer>
					<button special="cancel" class="oe_highlight" icon="fa-archive" string="Close" />
				</footer>
			</form>
		</field>
	</record>

</odoo>
