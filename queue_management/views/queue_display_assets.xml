<odoo>
    <template id="assets_backend" name="QMS assets" inherit_id="web.assets_backend">
        <!-- <xpath expr="." position="inside">
            <link rel="stylesheet" href="/queue_management/static/src/less/qm_dashboard.less"/>
        </xpath> -->
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" href="/queue_management/static/src/less/qms_dashboard.less"/>
            <link rel="stylesheet" href="/queue_management/static/src/less/qms_backend_dashboard.less"/>
        </xpath>
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/queue_management/static/src/js/dashboard.js"/>
        </xpath>
    </template>


    <template id="assets" name="queue_management assets">
        <script type="text/javascript" src="/queue_management/static/src/js/main.js"></script>
        <link rel="stylesheet" href="/queue_management/static/src/less/token_screen.less"/>
    </template>

    <template id="display_assets" name="queue_management assets">
        <script type="text/javascript" src="/queue_management/static/src/js/main.js"></script>
        <link rel="stylesheet" href="/queue_management/static/src/less/token_screen.less"/>
    </template>
    
</odoo>
